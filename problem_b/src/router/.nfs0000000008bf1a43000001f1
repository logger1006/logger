	for( int i=0; i<cNF.m_vForced.size(); i++ )
	{
		forced_C* cF = cNF.m_vForced[i];
		instance_C* pInst = cF->m_pInstance;
		if( !pInst->isMovable() )	
		{
			bHaveFixedCell = true;
			vF.push_back( cF );
		}
	}

	if( bHaveFixedCell )
	{
		for( int i=0; i<vF.size(); i++ )
		{
			forced_C* cF = vF[i];
			instance_C* pInst = cF->m_pInstance;
			nX = nX + pInst->getPlacedX();
			nY = nY + pInst->getPlacedY();
