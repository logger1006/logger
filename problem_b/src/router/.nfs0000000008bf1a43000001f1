inline int boundingBoxCost( int nTmp, vector<int> &vMin, vector<int> &vMax )
{
	int nCost = 0;
	int nSize = vMin.size();
	for( int i=0; i<nSize; i++ )
	{
		int nMin = min( nTmp, vMin[i] );
		int nMax = max( nTmp, vMax[i] );
		nCost = nCost + ( nMax - nMin + 1 );
	}
	return nCost;
}

