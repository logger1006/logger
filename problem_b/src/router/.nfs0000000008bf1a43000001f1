{
	
	//version 1
				
	int nDis = INT_MAX;
	for( int i=0; i<vTarget.size(); i++ )
	{
		nDis = min( nDis, nEularDistance( vTarget[i], pGrid2, pCurLayer ) );	
	}
	

	//version 1.5
	/*	
	int nDis = 0;
	for( int i=0; i<vTarget.size(); i++ )
	{
		nDis = nDis + nEularDistance( vTarget[i], pGrid2, pCurLayer );	
	}	
	*/	

	//version 2
	/*	
	int nDis = 0;
	vTarget.push_back( pGrid2 );

